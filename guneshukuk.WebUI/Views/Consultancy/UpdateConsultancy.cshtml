@model UpdateConsultancyDto;
@{
    ViewData["Title"] = "UpdateConsultancy";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<div class="content">
	<div class="container-fluid">
		<h4 class="page-title">HİZMET İŞLEMLERİ</h4>
		<div class="row">
			<div class="col-md-12">
				<form method="post">
					<div class="card">
						<div class="card-header">
							<div class="card-title">Hizmet Bilgi Girişi</div>
						</div>

						<div class="card-body">
							<input type="hidden" asp-for="ConsultancyId" />
							<div class="form-group">
								<label for="ConsultancyTitle">Hizmet Adı Giriniz</label>
								<input type="text" class="form-control" id="ConsultancyTitle" asp-for="ConsultancyTitle">
							</div>
							<div class="form-group">
								<label for="ConsultancyContenct">Hizmet Açıkmalası Giriniz</label>
								<input type="text" class="form-control" id="ConsultancyContent" asp-for="ConsultancyContent">
							</div>
							<div class="form-group">
								<label for="ImageUrl">Hizmet Resim Yolu Giriniz</label>
								<input type="text" class="form-control" id="ImageUrl" asp-for="ImageUrl">
							</div>
						</div>
						<div class="card-action">
							<button class="btn btn-primary">Hizmet Güncelle</button>
						</div>
					</div>
				</form>
			</div>
		</div>
	</div>
</div>


